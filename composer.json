{
    "name": "luomor/larke-sign-cert",
    "description": "Make some rsa, eddsa, ecdsa keys base on larke-admin.",
    "keywords": [
        "lake",
        "larke",
        "rsa",
        "eddsa",
        "ecdsa",
        "sign-cert",
        "admin",
        "larke-admin",
        "larke-admin-signcert"
    ],
    "homepage": "https://github.com/luomor-web/larke-admin-signcert",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Peter Zhang",
            "email": "zhangchunsheng423@gmail.com",
            "homepage": "https://github.com/zhangchunsheng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "luomor/larke-admin":"^1.4"
    },
    "autoload": {
        "psr-4": {
            "Larke\\Admin\\SignCert\\": "src"
        }
    },
    "extra" : {
        "larke": {
            "title": "签名证书",
            "description": "生成RSA,EdDSA,ECDSA,DSA等非对称签名证书",
            "keywords": [
                "rsa",
                "ecdsa",
                "eddsa",
                "SignCert"
            ],
            "homepage": "https://github.com/deatil/larke-admin-signcert",
            "authors": [
                {
                    "name": "deatil",
                    "email": "deatil@github.com",
                    "homepage": "https://github.com/deatil"
                }
            ],
            "version": "1.6.0",
            "adaptation": "^2.0",
            "require": {}
        },
        "laravel" : {
            "providers" : [
                "Larke\\Admin\\SignCert\\ServiceProvider"
            ]
        }
    }
}
